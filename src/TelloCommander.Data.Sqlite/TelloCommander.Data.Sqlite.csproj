<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>TelloCommander.Data.Sqlite</PackageId>
    <ReleaseVersion>1.0.0.5</ReleaseVersion>
    <PackageVersion>1.0.0.5</PackageVersion>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Dave Walker</Authors>
    <Copyright>Copyright (c) 2020 Dave Walker</Copyright>
    <DevelopmentDependency>true</DevelopmentDependency>
    <Owners>Dave Walker</Owners>
    <PackageReleaseNotes>First Release</PackageReleaseNotes>
    <Summary>TelloCommander Drone Telemetry SqlIte Database</Summary>
    <PackageTags>TelloCommander Tello Data Collector Memory Sqlite</PackageTags>
    <Title> TelloCommander Drone Telemetry SqlIte Database</Title>
    <Description>TelloCommander Drone Telemetry SqlIte Database</Description>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Class1.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\TelloCommander.Data\TelloCommander.Data.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="3.1.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="3.1.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="3.1.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="3.1.1">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Extensions.Configuration.FileExtensions" Version="3.1.1" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Content\TelloCommanderDbLicense.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Content\" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Content\TelloCommanderDbLicense.txt" />
  </ItemGroup>
</Project>
